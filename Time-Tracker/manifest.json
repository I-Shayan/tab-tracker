{
	"manifest_version": 3,
	"name": "Tab Tracker",
	"version": "1.0",
	"description": "Track time per site",
	"permissions": ["tabs", "storage", "notifications", "idle"],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "popup/popup.html"
	},
	"host_permissions": [
		"http://localhost:8888/*",
		"https://YOUR_NETLIFY_SITE.netlify.app/*"
	]
}
